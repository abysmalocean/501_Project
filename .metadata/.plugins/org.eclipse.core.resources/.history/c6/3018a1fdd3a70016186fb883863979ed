package vcu.cs531.Project;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;

public class ImageManuplation {
	
	BufferedImage img;
	
	public void LoadImageApp(String inputPath) {

	try{
		img = ImageIO.read(new File(inputPath));
		}catch(IOException e){
		e.printStackTrace();
		}
	}
}